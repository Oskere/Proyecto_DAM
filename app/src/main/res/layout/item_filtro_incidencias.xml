<!-- item_filtro.xml -->
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:padding="16dp">


    <TextView
        android:id="@+id/TextViewFiltro1"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:text="aaaaaaaaaaa"
        android:textColor="@color/texto"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.444" />

    <TextView
        android:id="@+id/TextViewFiltro2"
        android:layout_width="130dp"
        android:layout_height="30dp"
        android:layout_below="@id/TextViewFiltro1"
        android:layout_marginTop="4dp"
        android:text="aaaaaaaaaaaa"
        android:textColor="@color/texto"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.246"
        app:layout_constraintStart_toEndOf="@+id/TextViewFiltro1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.424" />

    <TextView
        android:id="@+id/textViewFiltro"
        android:layout_width="79dp"
        android:layout_height="43dp"
        android:layout_below="@id/TextViewFiltro1"
        android:layout_marginTop="4dp"
        android:text="aaaaaaaaaaaa"
        android:textColor="@color/texto"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/TextViewFiltro2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.625" />

</androidx.constraintlayout.widget.ConstraintLayout>
